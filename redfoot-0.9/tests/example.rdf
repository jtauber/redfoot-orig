<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
 xmlns:n1="http://redfoot.sourceforge.net/2000/09/24#"
 xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
  <rdf:Description rdf:ID="Car">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"></rdf:type>
    <rdfs:label>Car</rdfs:label>
    <rdfs:subClassOf rdf:resource="#Vehicle"></rdfs:subClassOf>
  </rdf:Description>
  <rdf:Description rdf:ID="James">
    <rdf:type rdf:resource="#Person"></rdf:type>
    <rdfs:label>James Tauber</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:ID="MyCar">
    <rdf:type rdf:resource="#Car"></rdf:type>
    <rdfs:label>My Car</rdfs:label>
    <n1:door>4</n1:door>
    <n1:color>Blue</n1:color>
  </rdf:Description>
  <rdf:Description rdf:ID="Person">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"></rdf:type>
    <rdfs:label>Person</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2000/01/rdf-schema#Resource"></rdfs:subClassOf>
  </rdf:Description>
  <rdf:Description rdf:ID="door">
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"></rdf:type>
    <rdfs:domain rdf:resource="#Vehicle"></rdfs:domain>
    <rdfs:label>door</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"></rdfs:range>
  </rdf:Description>
  <rdf:Description rdf:ID="Vehicle">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"></rdf:type>
    <rdfs:label>Vehicle</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2000/01/rdf-schema#Resource"></rdfs:subClassOf>
  </rdf:Description>
  <rdf:Description rdf:ID="color">
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"></rdf:type>
    <rdfs:domain rdf:resource="#Vehicle"></rdfs:domain>
    <rdfs:label>color</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"></rdfs:range>
  </rdf:Description>
  <rdf:Description rdf:ID="statedBy">
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"></rdf:type>
    <rdfs:domain rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Statement"></rdfs:domain>
    <rdfs:label>stated By</rdfs:label>
    <rdfs:range rdf:resource="#Person"></rdfs:range>
  </rdf:Description>
  <rdf:Description rdf:ID="statement1">
    <rdf:object>Red</rdf:object>
    <rdf:predicate rdf:resource="#color"></rdf:predicate>
    <rdf:subject rdf:resource="#MyCar"></rdf:subject>
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Statement"></rdf:type>
    <rdfs:label>My Statement</rdfs:label>
    <n1:statedBy rdf:resource="#James"></n1:statedBy>
  </rdf:Description>
</rdf:RDF>
