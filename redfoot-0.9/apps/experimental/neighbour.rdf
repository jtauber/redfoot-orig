<rdf:RDF
	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
	xmlns:b="http://redfoot.sourceforge.net/2000/10/06/builtin#"
>

  <rdf:Description rdf:ID="Neighbour">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdfs:label>Neighbour</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2000/01/rdf-schema#Resource"/>
  </rdf:Description>

  <rdf:Description rdf:ID="location">
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdfs:domain rdf:resource="#Neighbour"/>
    <rdfs:label>location</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
  </rdf:Description>

  <rdf:Description rdf:ID="systemID">
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdfs:domain rdf:resource="#Neighbour"/>
    <rdfs:label>systemID</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
  </rdf:Description>

  <rdf:Description rdf:ID="loadOnStartup">
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdfs:domain rdf:resource="#Neighbour"/>
    <rdfs:label>loadOnStartup</rdfs:label>
    <rdfs:range rdf:resource="http://redfoot.sourceforge.net/2000/10/06/builtin#YesNo"/>
  </rdf:Description>

  <rdf:Description rdf:ID="connected">
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdfs:domain rdf:resource="#Neighbour"/>
    <rdfs:label>connected</rdfs:label>
    <rdfs:range rdf:resource="http://redfoot.sourceforge.net/2000/10/06/builtin#YesNo"/>
  </rdf:Description>
  
</rdf:RDF>